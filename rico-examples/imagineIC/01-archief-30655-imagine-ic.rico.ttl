@prefix rdf:        <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:       <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema:     <https://schema.org/> .
@prefix rico:       <https://www.ica.org/standards/RiC/ontology#> .
@prefix blauwdruk:  <https://example.memorix-test.nl/resources/recordtypes/> .
@prefix voc:        <https://example.memorix-test.nl/vocabularies/> .
@prefix data:       <https://example.memorix-test.nl/resources/records/> .
@prefix xsd:        <http://www.w3.org/2001/XMLSchema#> .

<>
    a blauwdruk:Archief ;
    rico:hasAccumulator data:de-UUID-voor-record-stg-imagine-ic ;
    rico:hasOrHadIdentifier [
        a rico:Identifier ;
        rico:hasIdentifierType voc:de-UUID-voor-concept-toegangsnummer ; 
        rico:textualValue "30655"
    ] ;
    rico:hasOrHadTitle [
        a rico:Title ;
        rico:textualValue "Archief van Imagine IC"
    ] ;
    rico:hasRecordSetType <https://www.ica.org/standards/RiC/vocabularies/recordSetTypes#Fonds> ;
    rico:isAssociatedWithDate [
        a rico:DateRange ;
        rico:expressedDate "2001 - 2010" ;
        rico:hasBeginningDate [
            a rico:SingleDate ;
            rico:normalizedDateValue "2001"^^xsd:gYear
        ] ;
        rico:hasEndDate [
            a rico:SingleDate ;
            rico:normalizedDateValue "2010"^^xsd:gYear
        ] ;
    ] ;
    rico:isOrWasDescribedBy [
        a blauwdruk:Inventaris ;
        rico:hasDocumentaryFormType <https://www.ica.org/standards/RiC/vocabularies/documentaryFormTypes#FindingAid> ;
        rico:hasOrHadTitle [
            a rico:Title ;
            rico:textualValue "Inventaris van het Archief van Imagine IC"
        ] ;
        rico:hasPublisher data:de-UUID-voor-SAA ;
    ] .

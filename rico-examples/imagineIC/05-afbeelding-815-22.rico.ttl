@prefix rdf:        <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:       <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema:     <https://schema.org/> .
@prefix rico:       <https://www.ica.org/standards/RiC/ontology#> .
@prefix blauwdruk:  <https://example.memorix-test.nl/resources/recordtypes/> .
@prefix voc:        <https://example.memorix-test.nl/vocabularies/> .
@prefix data:       <https://example.memorix-test.nl/resources/records/> .
@prefix xsd:        <http://www.w3.org/2001/XMLSchema#> .

<>
    a blauwdruk:Afbeelding ;
    rico:hasOrHadIdentifier [
        a rico:Identifier ;
        rico:hasIdentifierType voc:de-UUID-voor-concept-inventarisnummer ; 
        rico:textualValue ""
    ] ;
    rico:hasOrHadTitle [
        a rico:Title ;
        rico:textualValue "Ingevlochten haar bij kinderen."
    ] ;
    rico:scopeAndContent "Foto voor de tentoonstelling 'Heur Haar. Black hairdo's in de Bijlmer' over zwarte jonge vrouwen die thuis elkaars haar doen, gemaakt in opdracht van Imagine IC" ;
    rico:isAssociatedWithDate [
        a rico:SingleDate ;
        rico:expressedDate "2004" ;
        rico:normalizedDateValue "2004"^^xsd:gYear
    ] ;
    rico:precedesOrPreceded <> ;
    rico:isOrWasIncludedIn <> ;
    rico:hasInstantiation <> .

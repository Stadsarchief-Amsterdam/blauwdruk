    rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
    rdfs:label "Mandate Relation"@en ;
    rdfs:subClassOf :RuleRelation, [
        a owl:Restriction ;
        owl:allValuesFrom :Agent ;
        owl:onProperty :authorizingAgent
    ], [

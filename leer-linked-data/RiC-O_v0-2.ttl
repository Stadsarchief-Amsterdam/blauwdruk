    rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
    rdfs:label "accumulation relation has source "@en ;
    rdfs:range [
        a owl:Class ;
        owl:unionOf (:Instantiation
            :RecordResource
        )
    ] ;
    rdfs:subPropertyOf :agentOriginationRelationHasSource ;
    owl:inverseOf :recordResourceOrInstantiationIsSourceOfAccumulationRelation .


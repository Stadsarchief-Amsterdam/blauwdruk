        a owl:Restriction ;
        owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
        owl:onClass :Person ;
        owl:onProperty :spouseRelationConnects
    ] ;
    :RiCCMCorrespondingComponent """Class implementation of RiC-R049 and RiC-R049i
            relations"""@en .

:TeachingRelation
    a owl:Class ;
    rdfs:comment """Connects at least one Person to at least another Person, who is
            their student."""@en ;
    rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
    rdfs:label "Teaching Relation"@en ;
